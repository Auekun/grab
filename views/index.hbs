<h1> Restaurant </h1>
<ul class="restaurant">
    {{#each restaurants}} 
    <il class="restaurant-item">
        <img src = "{{this.imageURL}}" class="restaurants-item-image">
        <div class="restaurant-item-name">{{this.name}}</div>
        <small class="restaurant-item-type"> {{this.type}}</small>
        </il>
        {{/each}}
        </ul>>